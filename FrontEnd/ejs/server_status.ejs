<div class="gray-bg">

	<div class="wrapper wrapper-content">
		<div id="regionSelectDiv"></div>

		<div class="row">
			<div class="col-lg-4">
				<div class="ibox float-e-margins">
					<div class="ibox-title">
						<span class="label label-success pull-right">Today</span>
						<h5>Network Input Traffic</h5>
					</div>
					<div class="ibox-content">
						<h1 class="no-margins" id="inputTrafficTotal">0 kb</h1>
						<div class="stat-percent font-bold text-success">
							0% <i class="fa fa-bolt"></i>
						</div>
						<small>Total input</small>
					</div>
				</div>
			</div>
			<div class="col-lg-4">
				<div class="ibox float-e-margins">
					<div class="ibox-title">
						<span class="label label-info pull-right">Today</span>
						<h5>Network Output Traffic</h5>
					</div>
					<div class="ibox-content">
						<h1 class="no-margins" id="outputTrafficTotal">0 kb</h1>
						<div class="stat-percent font-bold text-info">
							0% <i class="fa fa-level-up"></i>
						</div>
						<small>Total output</small>
					</div>
				</div>
			</div>

			<div class="col-lg-4">
				<div class="ibox float-e-margins">
					<div class="ibox-title">
						<span class="label label-danger pull-right">Today</span>
						<h5>Issue</h5>
					</div>
					<div class="ibox-content">
						<h1 class="no-margins" id="todayIssueCount">0</h1>
						<div class="stat-percent font-bold text-danger" id="compareYesterdayIssueCount">
						</div>
						<small>Yesterday</small>
					</div>
				</div>
			</div>
		</div>


		<div class="row">

			<div class="col-lg-8">
				<div class="ibox float-e-margins">
					<div class="ibox-title">
						<h5>Network Traffic</h5>
						<div class="pull-right">
							<span class="label label-navy pull-right" id="trafficStatusTime"></span>
						</div>
					</div>
					<div class="ibox-content">
						<div class="row">
							<div class="col-lg-12">
								<div id="trafficChart"></div>
							</div>
						</div>
					</div>
				</div>
			</div>

			<div class="col-lg-4">
				<div class="ibox float-e-margins">
					<div class="ibox-title">
						<h5>Hardware Status</h5>
						<div class="pull-right">
							<span class="label label-navy pull-right" id="hardwareStatusTime"></span>
						</div>
					</div>
					<div class="ibox-content">
						<div class="row">
							<div class="col-lg-6">
								<div id="cpuUsageChart"></div>
							</div>
							<div class="col-lg-6">
								<div id="memUsageChart"></div>
							</div>
						</div>
						<div class="row">
							<div class="col-lg-6">
								<div id="logDiskUsageChart"></div>
							</div>
							<div class="col-lg-6">
								<div id="osDiskUsageChart"></div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>


		<div class="row">
			<div class="col-lg-4">
				<div class="ibox float-e-margins">
					<div class="ibox-title">
						<h5>Failover Timeline</h5>
						<div class="ibox-tools">
							<a class="collapse-link"> <i class="fa fa-chevron-up"></i>
							</a> <a class="close-link"> <i class="fa fa-times"></i>
							</a>
						</div>
					</div>
					<div class="ibox-content ibox-heading">
						<h3>
							<i class="fa fa-envelope-o"></i> Today Failover
						</h3>
						<small id="failoverInfo"><i class="fa fa-tim"></i></small>
					</div>
					<div class="ibox-content">
						<div class="feed-activity-list" id="failoverHistoryDiv">
						</div>
					</div>
				</div>
			</div>

			<div class="col-lg-8">

				<div class="row">
					<div class="col-lg-6">
						<div class="ibox float-e-margins">
							<div class="ibox-title">
								<h5>Process Monitoring</h5>
								<div class="ibox-tools">
									<a class="collapse-link"> <i class="fa fa-chevron-up"></i>
									</a> <a class="close-link"> <i class="fa fa-times"></i>
									</a>
								</div>
							</div>
							<div class="ibox-content" id="processMonitoringDiv">
							</div>
						</div>
					</div>


					<div class="col-lg-6">
						<div class="ibox float-e-margins">
							<div class="ibox-title">
								<h5>INTERFACE STATUS</h5>
								<div class="ibox-tools">
									<a class="collapse-link"> <i class="fa fa-chevron-up"></i>
									</a> <a class="close-link"> <i class="fa fa-times"></i>
									</a>
								</div>
							</div>
							<div class="ibox-content" id="interFaceStatusDiv">
							</div>
						</div>
					</div>
				</div>
			</div>


		</div>
	</div>
</div>

<style>
#cpuUsageChart {
	width: 100%;
	height: 150px;
	font-size: 11px;
}

#memUsageChart {
	width: 100%;
	height: 150px;
	font-size: 11px;
}

#logDiskUsageChart {
	width: 100%;
	height: 150px;
	font-size: 11px;
}

#osDiskUsageChart {
	width: 100%;
	height: 150px;
	font-size: 11px;
}

#trafficChart {
	width: 100%;
	height: 300px;
	font-size: 11px;
}

</style>